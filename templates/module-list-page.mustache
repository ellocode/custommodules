<input type="hidden" id="hd-url" value="{{pageUrl}}" />
<a href="{{createUrl}}" class="btn btn-primary mb-2">Agregar</a>
<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
 {{#modules}}
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="heading-{{id}}">
     <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-8">
       <h4 class="panel-title">
        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-{{id}}" aria-expanded="false" aria-controls="collapse-{{id}}">
          {{fullname}}
        </a>
      </h4>
      </div>
      <div class="col-xs-6 col-md-4">
      <a href="{{edit_url}}"><span class="glyphicon glyphicon-cog" aria-hidden="true"></span></a> | 
      <a href="{{config_url}}"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a> | 
      <a href="" ><span class="glyphicon glyphicon-remove-sign delete" data-moduleid="{{id}}" aria-hidden="true"></span></a>
      </div>
     </div>
    </div>
    <div id="collapse-{{id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading-{{id}}">
      <div class="panel-body">
       <ul class="list-group">
        {{#courses}}
          <li class="list-group-item">{{coursename}}</li>
        {{/courses}}
        </ul>
      </div>
    </div>
  </div>
 {{/modules}}
</div>